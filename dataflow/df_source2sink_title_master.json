{
	"name": "df_source2sink_title_master",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_title_master",
						"type": "DatasetReference"
					},
					"name": "sourcetabletitlemaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_analytics_payroll_title_md",
						"type": "DatasetReference"
					},
					"name": "sinksqltitlemaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcetabletitlemaster",
